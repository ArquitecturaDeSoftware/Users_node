<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="checkUserService" targetNamespace="http://localhost:8001/checkUser" xmlns="http://localhost:8001/checkUser" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <wsdl:message name="checkUserRequest">
    <wsdl:part name="cedula" type="xs:string"/>
  </wsdl:message>

  <wsdl:message name="checkUserResponse">
    <wsdl:part name="cedula" type="xs:string"/>
    <wsdl:part name="name" type="xs:string"/>
    <wsdl:part name="lunchroom_id" type="xs:string"/>
    <wsdl:part name="active_ticket" type="xs:string"/>
